@model Models.ViewModels.CreateModel
@{
    Layout = null;
 }

<div style="padding:5px;">
    <div class="bg-primary m-1 p-1 text-white"><h4>Create User</h4></div>
    <div asp-validation-summary=" All" class="text-danger"></div>
    <form asp-action="Create" asp-controller="Admin" method="post" id="post-create-user">
        <div class="form-group">
            <label asp-for="Name"></label>
            <input asp-for="Name" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Email"></label>
            <input asp-for="Email" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Password"></label>
            <input asp-for="Password" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Code"></label>
            <input asp-for="Code" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="CodeId"></label>
            <input asp-for="CodeId" class="form-control" />
        </div>
        <div class="form-group">
            <label>@(Model.message??"")</label>
        </div>
        <button type="submit" class="btn btn-primary">Create</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cancel</button>
        <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
    </form>
</div>
