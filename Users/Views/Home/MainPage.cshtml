@model Models.ViewModels.LoginModel
@{
    //Layout = "_LayoutMain";
}
<style>
    ul {
        list-style-type: none;
        margin: 0;
        padding: 0;
        overflow: hidden;
    }

    li {
        float: left;
    }

        li a {
            display: block;
            padding: 1px 5px 1px 5px;
            background-color: white;
        }


    /*begin news-list */
    .newsfeed {
        height: 500px;
        overflow-y: scroll;
        padding-right: 20px;
    }

    .news-widget-title {
        font-size: 16px;
        font-size: 1rem;
        padding-bottom: 5px;
        border-bottom: 3px solid #e64946;
    }

    .clearfix {
        display: block;
    }

    ol, ul.news-class {
        list-style: none;
        padding-left: 10px;
        padding-right: 0px !important;
    }

    .news-custom-posts-thumb {
        float: left;
        margin-right: 15px;
    }

    .news-custom-posts-header {
        overflow: hidden;
    }

    .news-custom-posts-small-title {
        font-weight: 700;
        line-height: 1.3;
        font-size: 12px;
        height: 45px;
        overflow: hidden;
    }

    .news-custom-posts-small .news-meta {
        font-size: 11px;
        font-size: 0.6875rem;
    }

    .newsfeed img {
        /*max-width: 80px;*/
        height: auto;
    }

    img {
        max-width: 100%;
        width: auto\9;
        /*height: auto;*/
        vertical-align: bottom;
    }
    /*end news-list */

    .nav-pills > li > a.active {
        background-color: #ffffff;
        color: chartreuse;
    }

    .nav-pills > li > a:hover {
        color: chartreuse;
    }

    /*.nav-link-color {
        color: black;
        background-color: mediumaquamarine;
    }*/

    ul.nav-back-color2 li a {
        background-color: mediumaquamarine;
    }

        ul.nav-back-color2 li a:hover {
            color: black;
        }

    .center-content {
        position: absolute;
        margin: auto;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
    }

    .parent {
        background: yellow;
        margin: 20px;
        border: 1px solid black;
        position: relative;
        height: 500px;
    }

    .center {
        position: absolute;
        margin: auto;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
    }
</style>
<div style="width:1180px;  background-color:mediumaquamarine; position:relative;float:left;">
    <ul class="nav nav-pills nav-back-color2">
        <li class="nav-item">
            <a asp-action="SecurePage" asp-controller="Home">Secure Page</a>
        </li>
        <li class="nav-item">
            <a asp-action="SecurePage1" asp-controller="Home">News</a>
        </li>
        <li class="nav-item">
            <a asp-action="PaginationPage" asp-controller="Home">Pagination Page</a>
        </li>
        <li class="nav-item">
            <a asp-action="PaginationPage_a" asp-controller="Home">Pagination Page A</a>
        </li>
        <li class="nav-item">
            <a asp-action="Heritage" asp-controller="Heritage">Heritage A</a>
        </li>
        <li class="nav-item">
            <a href="#contact">Contact</a>
        </li>
        <li class="nav-item">
            <a href="#about">About</a>
        </li>
    </ul>
</div>
<div style="width:1180px; height:680px; background-color:azure; position:relative;float:left;">
    <div style="width:830px; height:100%; background-color:chartreuse; position:relative;float:left;"></div>
    <div style="width:350px; height:100%; background-color:cadetblue; position:relative;float:left;">
        <div style="width:350px;height:500px; background-color:white; position:relative;float:left;">
            <div class="newsfeed" style="">
                <h4 class="news-widget-title">
                    <span class="news-widget-title-inner"><a href="/2019/03/14">Լրահոս</a></span>
                </h4>
                <ul id="newsfeedList" class="news-custom-posts-widget clearfix news-class" news-img-list="aaa" info="TagModel"></ul>
            </div>
        </div>
    </div>
</div>
